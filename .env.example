# Blockchain Explorer API Key
# Etherscan API V2 - Single key works for all 50+ EVM chains!
# Get your key at: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Price Data APIs
# DeFiLlama is FREE and requires NO API key!
# If you want to use CoinGecko Pro instead, add your key here (optional)
COINGECKO_API_KEY=

# Optional: Moralis/Alchemy (alternative to individual chain APIs)
# MORALIS_API_KEY=your_moralis_api_key_here
# ALCHEMY_API_KEY=your_alchemy_api_key_here

# Cache Settings
CACHE_TTL_TRANSACTIONS=600  # 10 minutes in seconds
CACHE_TTL_PRICES=120        # 2 minutes in seconds

# Agent Settings
PORT=3000
NODE_ENV=development
